doi = {10.1109/TPEL.2005.869757},
isbn = {0885-8993},
issn = {08858993},
journal = {IEEE Transactions on Power Electronics},
keywords = {Active harmonic elimination,Field programmable gate array (FPGA) controller,Multilevel converter},
number = {2},

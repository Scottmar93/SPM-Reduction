journal = {Journal of Power Sources},
keywords = {Aging mechanism,Calendar life,Capacity fading,Lithium-manganese spinel,Lithium-nickel-cobalt oxide},
number = {1-2},
pages = {58--64},
title = {{Aging mechanisms of lithium cathode materials}},
volume = {127},
year = {2004}
}

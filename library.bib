journal = {Journal of Power Sources},
keywords = {Battery capacity,Battery cycle-life,Battery power,Capacity fade,Lithium-ion batteries,Power fade},
month = {jun},
pages = {865--869},
title = {{Power fade and capacity fade resulting from cycle-life testing of advanced technology development program lithium-ion batteries}},

isbn = {0022-5096},
journal = {Journal of the Mechanics and Physics of Solids},
keywords = {anodes,batteries,bulk,chemo-mechanical processes,diffusion,elastic-viscoplastic material,electro-mechanical processes,electrode particles,evolution,in-situ measurements,li,nanoscale olivines,phase-transformation,silicon,storage},

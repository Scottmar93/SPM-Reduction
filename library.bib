doi = {10.1016/j.electacta.2016.05.136},
file = {:home/marquis/Dropbox/InFomm/siemens/papers/Electrode Particles/Deformation/Chen, Zhou, Cai - 2016 - Analytical model for crack propagation in spherical nano electrodes of lithium-ion batteries.pdf:pdf},

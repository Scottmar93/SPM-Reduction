doi = {10.1149/2.024203jes},
file = {:home/marquis/Dropbox/InFomm/siemens/papers/Binder/Binder Properties/Liu et al. - Particles and polymer binder interaction. A controlling factor in lithium-ion electrode performance.pdf:pdf},

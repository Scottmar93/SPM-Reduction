journal = {Journal of The Electrochemical Society},
number = {3},
pages = {A456----A462},
publisher = {The Electrochemical Society},
title = {{Solvent diffusion model for aging of lithium-ion battery cells}},
url = {http://jes.ecsdl.org/cgi/doi/10.1149/1.1644601},
volume = {151},

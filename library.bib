@article{Pop2007,
abstract = {Li-ion is currently the most commonly used battery chemistry in portable{\$}\backslash{\$}napplications. Accurate state-of-charge SOC and{\$}\backslash{\$}n{\$}\backslash{\$}nremaining run-time indication for portable devices is important for{\$}\backslash{\$}nuser convenience and to prolong the lifetime of batteries. The{\$}\backslash{\$}n{\$}\backslash{\$}nactual SOC algorithms, which the main companies use in practice, make{\$}\backslash{\$}nuse of the so-called electromotive force emf. In these{\$}\backslash{\$}n{\$}\backslash{\$}nSOC systems it is assumed that the emf of an Li-ion battery only depends{\$}\backslash{\$}non aging to a limited extent. In this paper, novel emf{\$}\backslash{\$}n{\$}\backslash{\$}nmeasurement and modeling efforts are presented as a function of battery{\$}\backslash{\$}naging. As will be shown, a better understanding of this{\$}\backslash{\$}n{\$}\backslash{\$}ndependence is useful for improving SOC accuracy.},

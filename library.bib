keywords = {Multilevel converter,Selective harmonic elimination (SHE),Voltage balancing control},
number = {6},
pages = {2946--2953},
title = {{A complete harmonic elimination approach to DC link voltage balancing for a cascaded multilevel rectifier}},
url = {http://ieeexplore.ieee.org/xpls/abs{\_}all.jsp?arnumber=4373444},
volume = {54},
year = {2007}
}
@article{Lee2014,

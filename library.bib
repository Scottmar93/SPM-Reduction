issn = {0197-2618},
keywords = {-motor overload protection,adaptive kalman,filtering,hybrid thermal model,induction machines,input estimation,noise identification,temperature estimation},
pages = {2--9},
title = {{An adaptive Kalman filtering approach to induction machine stator winding temperature estimation based on a hybrid thermal model}},

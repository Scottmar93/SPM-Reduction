doi = {10.1149/1.1872737},
file = {:home/marquis/Dropbox/InFomm/siemens/papers/Battery Models/Porous Electrode Model/Model Parameters/Valoen et al. - Transport Properties of LiPF based li-ion battery electrolytes.pdf:pdf},

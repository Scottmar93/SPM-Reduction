pages = {1349--1357},
title = {{Numerical simulation for the discharge behaviors of batteries in series and/or parallel-connected battery pack}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0013468606007717},
volume = {52},
year = {2006}
}
